    <!-- ////////// MANUALLY TRIGGERED TAKEOVERS \\\\\\\\\\ -->
    <ScriptedEvent identifier="startmanualtakeover">
      <TagAction criteria="player" tag="player" ContinueIfNoTargetsFound="true" />
      <TagAction criteria="humanprefabidentifier:outpostmanagercoalition" tag="coalitionstation" ContinueIfNoTargetsFound="true" />
      <TagAction criteria="humanprefabidentifier:outpostmanagerseparatists" tag="separatiststation" ContinueIfNoTargetsFound="true" />
      <TagAction criteria="humanprefabidentifier:outpostmanagerpirate" tag="banditstation" ContinueIfNoTargetsFound="true" />
      <TagAction criteria="humanprefabidentifier:outpostmanagerhusk" tag="churchstation" ContinueIfNoTargetsFound="true" />
      <CheckConditionalAction targettag="coalitionstation">
        <Conditional isDead="true" />
        <Success>
          <CheckConditionalAction targettag="separatiststation">
            <Conditional isDead="true" />
            <Success>
              <CheckConditionalAction targettag="banditstation">
                <Conditional isDead="true" />
                <Success>
                  <CheckConditionalAction targettag="churchstation">
                    <Conditional isDead="true" />
                    <Success>
                      <ConversationAction text="According to the tracker you're not even at a station! Don't waste our time!" dialogtype="Small" targettag="player" eventsprite="coalition" ContinueAutomatically="true" />
                    </Success>
                    <Failure>
                      <ConversationAction text="You're at a station of the church of husk. We have no intend on interferring with them. Permission denied!" dialogtype="Small" targettag="player" eventsprite="coalition" ContinueAutomatically="true" />
                    </Failure>
                  </CheckConditionalAction>
                </Success>
                <Failure>
                  <ConversationAction text="You're at a pirate station. We are not interested in their station. Do whatever, but we will not claim the station!" dialogtype="Small" targettag="player" eventsprite="coalition" ContinueAutomatically="true" />
                </Failure>
              </CheckConditionalAction>
            </Success>
            <Failure>
              <ConversationAction text="You're calling the coalition over the military frequency..." dialogtype="Small" targettag="player" eventsprite="coalition">
                <Option text="We'd like to take over a station for you..." endconversation="true">
                  <!-- <CheckReputationAction targettype="faction" identifier="coalition" condition="lt -60">
                    <Success> -->
                      <TriggerEventAction identifier="manualcooutposttakeoverevent" />
                    <!-- </Success>
                    <Failure>
                      <ConversationAction text="We're currently lacking the capacity to manage that station. Permission denied!" dialogtype="Small" targettag="player" eventsprite="coalition" ContinueAutomatically="true" />
                    </Failure>
                  </CheckReputationAction> -->
                </Option>
                <Option text="Nevermind" endconversation="true" />
              </ConversationAction>
            </Failure>
          </CheckConditionalAction>
        </Success>
        <Failure>
          <ConversationAction text="You're calling the separatists over the military frequency..." dialogtype="Small" targettag="player" eventsprite="separatists">
            <Option text="We'd like to take over a station for you..." endconversation="true">
              <!-- <CheckReputationAction targettype="faction" identifier="separatists" condition="lt -60">
                <Success> -->
                  <TriggerEventAction identifier="manualsepoutposttakeoverevent" />
                <!-- </Success>
                <Failure>
                  <ConversationAction text="We're currently lacking the capacity to manage that station. Permission denied!" dialogtype="Small" targettag="player" eventsprite="coalition" ContinueAutomatically="true" />
                </Failure>
              </CheckReputationAction> -->
            </Option>
            <Option text="Nevermind" endconversation="true" />
          </ConversationAction>
        </Failure>
      </CheckConditionalAction>
    </ScriptedEvent>

